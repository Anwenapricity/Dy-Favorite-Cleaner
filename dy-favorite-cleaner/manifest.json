{
  "manifest_version": 3,
  "name": "抖音音乐一键取消收藏",
  "version": "1.0",
  "description": "在抖音音乐收藏页面添加一键取消收藏按钮",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "*://*.douyin.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.douyin.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
} 